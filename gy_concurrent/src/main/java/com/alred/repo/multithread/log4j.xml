<?xmlversion="1.0"encoding="utf-8"?>
<configuration>

    <properties>
        <!--文件输出格式-->
        <property name="PATTERN">%d{yyyy-MM-ddHH:mm:ss.SSS}[%t]%-5level%logger{36}-%msg%n
        </property>
    </properties>

    <appenders>
        <Console name="CONSOLE" target="system_out">
            <PatternLayout pattern="${PATTERN}"/>
        </Console>
    </appenders>

    <loggers>
        <root level="info">
            <appenderref ref="CONSOLE"/>
        </root>
    </loggers>

</configuration>